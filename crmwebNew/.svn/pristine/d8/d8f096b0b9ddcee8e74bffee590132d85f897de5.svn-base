.search_field_comp {
    display: inline-block;
    margin: 0px 10px;
    height: 35px;
    padding: 0px 5px;
    border-radius: 5px;
    border: 1px solid #ccc;
}

.ui-jqgrid-pager .ui-paging-info {
    margin-right: 20px !important;
