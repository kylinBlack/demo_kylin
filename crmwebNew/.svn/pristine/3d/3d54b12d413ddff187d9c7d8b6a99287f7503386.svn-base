/***************************相关项设置部分样式************************/
#content-wrapper-dialog{
    padding: 24px;
}
.list {
    overflow-y: auto;
}
.list-head {
    height: 30px;
    line-height: 30px;
    border-bottom: thin solid #ccc;
    margin-bottom: 10px;
}
.list-head label:first-child {
    width: 50px;
    float: left;
}

.optinal-warp .list-head font {
    display: inline-block;
    text-align: center;
    color: #999999;
    height: 28px;
    line-height: 28px;
    padding: 0 5px;
    cursor: pointer;
}
.optinal-warp .list-head .active {
    color: black;
    border-bottom: 3px solid #0099FF;
}

ul {
    list-style-type: none;
    margin: 0;
    padding: 0;
}
.bg-optinal, .bg-selected {
    display: inline-block;		/*触发 hasLayout*/
    background-color: #FFFFFF;
    margin-right: 2px;
    margin-bottom: 6px;			/*float会导致margin-bottom无效*/
    border: thin solid #999;
    border-radius: 4px;
    float: left;
}

#selectedList {
    height: 114px;
    /*overflow-y: auto;*/
}
#selectedList li{
    color: black;
    margin: 0 3px 6px;
}
#optinalList {
    height: 150px;
    /*overflow-y: auto;*/
}
#optinalList li{
    color: #999;
    margin: 0 3px 6px;
}
.ui-sortable-placeholder {
    width: 60px;
    height: 30px;
}

.assignUser {
    color: #999;
}
.footer-warp button.btn {
    min-width: 80px;
}

.assign-warp {
    height: 30px;
    padding-left: 0;
}

.assign-warp #applyToAll {
    display: inline;
}
.applyDiv-warp {
    height: 30px;
    overflow-y: auto;
}
.assign-warp .assign-input {
    display: none;
}
.assign-warp .backApply-warp {
    height: 30px;
    line-height: 30px;
}
.assign-warp .backApply-warp #backApply {
    color: #0099FF;
    cursor: pointer;
}
.settingBtn {
    display: inline-block;
    margin-bottom: 0;
    font-weight: normal;
    text-align: center;
    vertical-align: middle;
    cursor: pointer;
    background-image: none;
    /*border: 1px solid transparent;*/
    white-space: nowrap;
    padding: 6px 12px;
    font-size: 12px;
    line-height: 1.42857143;
    border-radius: 2px;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}
