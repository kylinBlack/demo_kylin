.title {
  padding-bottom: 10px;
}
.title span {
  color: #a0a0a0;
}
.wrap {
  width: 300px;
  height: 780px;
  background-color: #f6f6f6;
  border-radius: 4px 4px 4px 4px;
  border: solid 1px #d4dee5;
  padding: 10px;
}
.wrap .task {
  padding-left: 5px;
}
.wrap .task p {
  font-size: 16px;
  color: #4d7394;
  padding-bottom: 10px;
}
.wrap .task .taskNum {
  margin-bottom: 10px;
  width: 260px;
  padding: 8px 10px 8px 20px;
  background-color: #fcfcfc;
  border: solid 1px #dde2e5;
  border-left: 5px solid #35bf8d;
}
.wrap .task .taskNum .time {
  line-height: 30px;
  padding: 5px 10px;
  color: #fff;
  background-color: #ec7161;
  border-radius: 11px 11px 11px 11px;
}
.wrap .task .addArea .addTask {
  padding-left: 10px;
  color: #57a3f2;
}
.wrap .task .modal-content {
  margin-top: 100px;
  width: 624px;
}
.wrap .task .modal-content .modal-body {
  padding: 0;
}
.wrap .task .modal-content .modal-body .leftMenu {
  width: 124px;
  background-color: red;
  float: left;
}
.wrap .task .modal-content .modal-body .rightContent {
  width: 500px;
  background-color: yellow;
  float: right;
}
.right-wrap {
  top: 40px;
  right: -910px;
}
.right-wrap-show {
  right: 0;
}
.taskDetail {
  width: 910px;
  line-height: 20px;
  padding-right: 0px;
}
.taskDetail .left {
  float: left;
  width: 570px;
}
.taskDetail .left .detail .correlation {
  border-bottom: 1px solid #d9e3eb;
}
.taskDetail .left .detail .correlation p {
  color: #4d6b8c;
  font-size: 16px;
  padding: 10px 0;
}
.taskDetail .left .detail .correlation .question {
  padding-bottom: 10px;
}
.taskDetail .left .detail .correlation .closingDate {
  padding-bottom: 10px;
}
.taskDetail .left .detail .correlation .closingDate .time {
  display: inline-block;
  padding: 3px 8px;
  color: #fff;
  background-color: #ec7161;
  border-radius: 11px 11px 11px 11px;
}
.taskDetail .left .detail .correlation .whom {
  padding-bottom: 10px;
}
.taskDetail .left .detail .correlation .whom .whomPerson {
  display: inline-block;
  padding: 6px 15px;
  color: #fff;
  background-color: #44cacb;
  border-radius: 15px 15px 15px 15px;
}
.taskDetail .left .detail .correlation .turn {
  padding-left: 12px;
  padding-bottom: 15px;
}
.taskDetail .left .main {
  background-color: #fff;
  min-height: 900px;
  width: 570px;
  float: left;
  position: relative;
}
.taskDetail .left .main .icon_area {
  position: absolute;
  right: 20px;
  top: 20px;
  width: 216px;
  height: 25px;
}
.taskDetail .left .main .icon_area span {
  display: inline-block;
  padding-right: 8px;
}
.taskDetail .left .main .icon_area span .goodImg {
  padding-right: 5px;
}
.taskDetail .left .main .icon_area span .messageImg {
  padding-right: 5px;
}
.taskDetail .left .main .mainWrap {
  padding: 10px 10px 10px 0px;
}
.taskDetail .left .main .mainWrap .title {
  color: #48a0dc;
  font-size: 14px;
  padding-left: 20px;
}
.taskDetail .left .main .mainWrap .questionText {
  padding-left: 20px;
}
.taskDetail .left .main .mainWrap .questionText .text {
  color: #345870;
}
.taskDetail .left .main .mainWrap .questionText .infos {
  padding: 15px 0px 10px;
}
.taskDetail .left .main .mainWrap .questionText .infos .level {
  color: #999999;
  padding-right: 100px;
}
.taskDetail .left .main .mainWrap .questionText .infos .status {
  color: #999999;
}
.taskDetail .left .main .mainWrap .questionText .classification {
  padding: 5px 0px 5px;
}
.taskDetail .left .main .mainWrap .questionText .classification span {
  display: inline-block;
  margin-right: 3px;
  width: 50px;
  height: 22px;
  text-align: center;
  line-height: 22px;
  border-radius: 12px;
  border: 1px solid #44cacb;
  background-color: #eeeeee;
}
.taskDetail .left .main .mainWrap .questionText .classification .active {
  background-color: #44cacb;
  color: #fff;
}
.taskDetail .left .main .mainWrap .issueDesc {
  padding-left: 20px;
  line-height: 20px;
}
.taskDetail .left .main .mainWrap .issueDesc .classificationHistory {
  padding: 10px 0px 10px;
}
.taskDetail .left .main .mainWrap .issueDesc .classificationHistory span {
  display: inline-block;
  margin-right: 3px;
  width: 50px;
  height: 22px;
  text-align: center;
  line-height: 22px;
  border-radius: 12px;
  border: 1px solid #44cacb;
  background-color: #eeeeee;
}
.taskDetail .left .main .mainWrap .instructions {
  border-bottom: 1px solid #eee;
  padding-bottom: 10px;
  padding-left: 20px;
}
.taskDetail .left .main .mainWrap .instructions div {
  line-height: 20px;
}
.taskDetail .left .main .mainWrap .btn-direction {
  padding-left: 20px;
}
.taskDetail .left .main .mainWrap .btn-type {
  padding-left: 45px;
}
.taskDetail .left .main .mainWrap .reply {
  padding-left: 20px;
}
.taskDetail .left .main .mainWrap .reply .replyTip {
  color: #444;
  font-size: 14px;
  font-weight: bold;
  padding: 8px 0px;
}
.taskDetail .left .main .mainWrap .reply .replyInfo {
  padding-bottom: 8px;
}
.taskDetail .left .main .mainWrap .reply .replyInfo .replyUser {
  display: inline-block;
  padding-right: 20px;
  color: #2f91d3;
  font-size: 14px;
  font-weight: bold;
}
.taskDetail .left .main .mainWrap .reply .replyInfo .replyTime {
  color: #999;
}
.taskDetail .left .main .mainWrap .reply .replyInfo img {
  float: right;
}
.taskDetail .left .main .mainWrap .reply .textareaFirst {
  outline: none;
  resize: none;
  width: 570px;
  border-radius: 4px 4px 4px 4px;
  border: solid 1px #d4dee5;
  height: 50px;
}
.taskDetail .left .main .mainWrap .reply .dismissal {
  color: #444;
  font-size: 14px;
  font-weight: bold;
  padding: 10px 0px 5px;
}
.taskDetail .left .main .mainWrap .reply .transfer {
  padding: 20px 0 0 0;
  line-height: 20px;
}
.taskDetail .left .main .mainWrap .reply .transfer .transferWhom {
  color: #444;
  font-size: 14px;
  font-weight: bold;
  padding: 8px 8px;
}
.taskDetail .left .main .mainWrap .reply .transfer img {
  padding-right: 5px;
  vertical-align: middle;
}
.taskDetail .left .main .mainWrap .reply .transfer .choose {
  color: #999;
}
.taskDetail .left .main .mainWrap .reply .buttons {
  padding-top: 120px;
  text-align: center;
}
.taskDetail .left .main .mainWrap .reply .buttons span {
  width: 100px;
  height: 35px;
  color: #fff;
  font-size: 14px;
}
.taskDetail .left .main .mainWrap .reply .buttons .btn-next {
  background-color: #48a0dc;
}
.taskDetail .left .main .mainWrap .reply .buttons .btn-sloved {
  background-color: #31be91;
}
.taskDetail .left .main .mainWrap .reply .buttons .btn-not {
  background-color: #f16957;
}
.right {
  float: right;
  width: 280px;
  min-height: 1000px;
  background-color: #dfe7ed;
  padding: 10px 20px 20px 20px;
}
.right .trail {
  font-size: 14px;
  color: #444444;
  margin-bottom: 20px;
}
.right .taskList .up {
  margin-top: -4px;
}
.right .taskList .up .status {
  display: inline-block;
  width: 10px;
  height: 10px;
  top: 28%;
  border-radius: 50%;
  background-color: #92bcd9;
}
.right .taskList .up .status i {
  position: absolute;
  height: 20px;
  border-right: 1px solid #c4d3dd;
}
.right .taskList .up .taskDate {
  font-size: 12px;
  color: #4090c8;
  margin-left: 10px;
}
.right .taskList #firstTask {
  border: 0px;
}
.right .taskList .infos {
  margin-left: 20px;
  width: 216px;
  height: 28px;
  line-height: 28px;
  padding-left: 10px;
  background-color: #ffffff;
  border-radius: 4px 4px 4px 4px;
  margin-bottom: 10px;
}
